{
    "name": "ballyhoo-technic/openshift-client",
    "description": "Simple OpenShift client for PHP. Fork of: https://github.com/universityofadelaide/openshift-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Barton",
            "email": "c.barton@adelaide.edu.au"
        },
        {
            "name": "Casey Fulton",
            "email": "casey.fulton@adelaide.edu.au"
        },
        {
            "name": "Rene Mertins",
            "email": "r.mertins@ballyhoo.de"
        }
    ],
    "require": {
        "php": ">=8",
        "guzzlehttp/guzzle" : "^7.4.5",
        "symfony/serializer": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "mockery/mockery" : "^1.5",
        "php-coveralls/php-coveralls": "^2.5.2"
    },
    "autoload": {
        "psr-4": {
            "UniversityOfAdelaide\\OpenShift\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "UniversityOfAdelaide\\OpenShift\\Tests\\": "tests/src"
      }
    }
}
